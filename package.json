{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development GENERATE_ENV=development nuxt dev",
    "dev-no-axe": "cross-env NODE_ENV=dev-no-axe GENERATE_ENV=development nuxt dev",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate:deploy": "cross-env GENERATE_ENV=production NODE_OPTIONS=--max_old_space_size=2048 nuxt build --fail-on-page-error && yarn functions:build",
    "generate:dev": "cross-env GENERATE_ENV=development NODE_OPTIONS=--max_old_space_size=2048 nuxt build --fail-on-page-error && yarn functions:build",
    "generate": "eslint \"./**/*.{js,ts,vue}\" && nuxt build",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:fix": "eslint \"./**/*.{js,ts,vue}\" --fix",
    "stylelint": "stylelint '**/*.{css,scss,vue}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,vue}'",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source",
    "functions:dev": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions"
  },
  "lint-staged": {
    "*.{js,ts,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./pre-commit.sh && lint-staged"
    }
  },
  "browser": {
    "[module-name]": false
  },
  "dependencies": {
    "axe-core": "4.1.3",
    "chart.js": "2.9.4",
    "cross-env": "7.0.3",
    "dayjs": "1.10.4",
    "netlify-lambda": "2.0.3",
    "nuxt": "2.15.3",
    "nuxt-i18n": "6.21.0",
    "vue": "2.6.12",
    "vue-chartjs": "3.5.1",
    "vue-meta": "2.4.0",
    "vue-router": "4.0.5",
    "vue-scrollto": "2.20.0",
    "vue-spinner": "1.0.4",
    "vuetify": "3.0.0-alpha.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime-corejs3": "7.13.10",
    "@mdi/js": "5.9.55",
    "@nuxt/types": "2.15.3",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config": "6.0.0",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "2.0.0-beta.2",
    "@types/chart.js": "2.9.31",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "@vue/compiler-core": "3.0.7",
    "@vue/compiler-dom": "3.0.7",
    "@vue/compiler-sfc": "3.0.7",
    "@vue/server-renderer": "3.0.7",
    "babel-loader": "8.2.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "core-js": "3.9.1",
    "css-loader": "4.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.11",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-svg-loader": "1.2.0",
    "nuxt-webfontloader": "1.1.0",
    "postcss-loader": "5.2.0",
    "precss": "4.0.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sass": "1.32.8",
    "sass-loader": "10.1.1",
    "schema-utils": "3.0.0",
    "style-loader": "2.0.0",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "21.0.0",
    "stylelint-scss": "3.19.0",
    "ts-loader": "8.0.18",
    "typescript": "4.2.3",
    "ufo": "0.6.10",
    "vue-axe": "2.4.4",
    "vue-server-renderer": "0.1.0",
    "vue-template-compiler": "0.1.0",
    "webpack-merge": "5.7.3",
    "workbox-webpack-plugin": "6.1.1"
  },
  "optionalDependencies": {
    "nuxt3": "0.0.55",
    "vue": "3.0.7"
  }
}
